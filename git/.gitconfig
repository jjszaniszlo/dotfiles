	email = Johnszaniszlo@gmail.com

[pull]
	rebase = false

[init]
	defaultBranch = master

[core]
  editor = vim

[color]
  ui = true

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = red
