# Mac specific zsh
alias ec="emacsclient -nc"

if type brew &>/dev/null; then
    FPATH=$(brew --prefix)/share/zsh-completions:$FPATH
    autoload -Uz compinit
    compinit
fi

[[ ! -r '/Users/jjszaniszlo/.opam/opam-init/init.zsh' ]] || source '/Users/jjszaniszlo/.opam/opam-init/init.zsh' > /dev/null 2> /dev/null
